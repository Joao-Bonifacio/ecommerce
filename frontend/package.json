{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --fix",
    "test": "dotenv -e .env.test -- vitest run",
    "test:watch": "dotenv -e .env.test -- vitest",
    "test:unit": "dotenv -e .env.test -- vitest run --exclude src/**/*.test.ts",
    "test:integration": "dotenv -e .env.test -- vitest run --exclude src/**/*.spec.ts*",
    "test:storybook": "test-storybook",
    "test:e2e": "dotenv -e .env.test -- cypress run",
    "test:cov": "dotenv -e .env.test -- vitest run --coverage --no-file-parallelism",
    "test:all": "npm run test:storybook && npm run test:unit && npm run test:integration && npm run test:e2e",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@t3-oss/env-nextjs": "^0.13.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.0",
    "zod": "^3.25.48",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.0.0",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.3",
    "@rocketseat/eslint-config": "^2.2.2",
    "@storybook/addon-a11y": "^9.0.4",
    "@storybook/addon-docs": "^9.0.4",
    "@storybook/addon-onboarding": "^9.0.4",
    "@storybook/addon-vitest": "^9.0.4",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs": "^9.0.4",
    "@storybook/nextjs-vite": "^9.0.4",
    "@storybook/react-vite": "^9.0.4",
    "@storybook/test-runner": "^0.22.0",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/storybook__react": "^5.2.1",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/browser": "^3.2.0",
    "@vitest/coverage-v8": "^3.2.0",
    "cypress": "^14.4.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-storybook": "^9.0.4",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "storybook": "^9.0.4",
    "supertest": "^7.1.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "tw-animate-css": "^1.3.3",
    "typescript": "^5",
    "vitest": "^3.2.0",
    "vitest-preview": "^0.0.1"
  },
  "overrides": {
    "storybook": "$storybook"
  }
}
